{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "required": [],
  "properties": {
    "firstName": {
      "type": "string",
      "default": "",
      "examples": ["JC"]
    },
    "lastName": {
      "type": "string",
      "default": "",
      "examples": ["Ketterer"]
    },
    "email": {
      "type": "string",
      "format": "email",
      "default": "",
      "examples": ["jketterer@CVNA.com"]
    },
    "hireDate": {
      "type": "string",
      "format": "date"
    },
    "milestone": {
      "type": "string",
      "default": "",
      "examples": ["M0"]
    },
    "current_milestone_start_date": {
      "type": "string",
      "format": "date"
    },
    "teamLead": {
      "type": "string",
      "default": "",
      "examples": ["Eileen Fronde"]
    },
    "manager": {
      "type": "string",
      "default": "",
      "examples": ["Tom Class"]
    }
  },
  "examples": [
    {
      "firstName": "JC",
      "lastName": "Ketterer",
      "email": "jketterer@CVNA.com",
      "hireDate": "2020-01-01",
      "milestone": "M0",
      "current_milestone_start_date": "2022-04-01",
      "teamLead": "Eileen Fronde",
      "manager": "Tom Class"
    }
  ],
  "additionalProperties": false
}
